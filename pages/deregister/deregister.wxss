/* pages/deregister/deregister.wxss */
.container{
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.deregister {
  display: inline-block;
  height:40px;
  width: 130px;
  line-height:40px;
  color: #E86262;
  font-size: 20px;
  text-align: center;
  border-radius: 15px;
  background-color: white;
  border: 1px solid rgba(0, 0, 0, 0.1);
}

.deregister image {
  margin-right: 6rpx;
  position: relative;
  top: 10rpx;
  display: inline-block;
  width: 26px !important;
  height: 26px !important;
}